{
  "self": {
    "name": "datafile-collector",
    "version": "1.0.0",
    "description": "Docker application to collect log file from PNF",
    "component_type": "docker"
  },
  "streams": {
    "subscribes": [
    ],
    "publishes": [
      {
        "type": "data_router",
        "config_key": "PM_MEAS_FILES",
        "format": "3GPP_XML",
        "version": "1.0.0"
      }
    ]
  },
  "services": {
    "calls": [],
    "provides": []
  },
