      "policy_editable": false,
      "type": "string",
      "required": true
    },
    {
      "name": "dmaap.dmaapConsumerConfiguration.consumerId",
      "value": "C12",
      "description": "",
      "designer_editable": true,
      "sourced_at_deployment": true,
      "policy_editable": false,
      "type": "string",
      "required": true
    },
    {
      "name": "dmaap.dmaapConsumerConfiguration.timeoutMs",
      "value": -1,
      "description": "",
      "designer_editable": true,
      "sourced_at_deployment": false,
      "policy_editable": false,
      "required": true
    }
  ]
}
